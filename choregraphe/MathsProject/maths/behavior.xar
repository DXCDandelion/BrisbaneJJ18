		elif(p == self.typeConversion(90)):
			self.output_10(p)
		elif(p == self.typeConversion(100)):
			self.output_11(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
		except:
			p = str(p)
		return p]]>
